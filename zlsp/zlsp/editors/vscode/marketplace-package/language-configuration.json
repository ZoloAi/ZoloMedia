{
  "comments": {
    "lineComment": "#"
  },
  "autoClosingPairs": [
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "'",
      "close": "'"
    }
  ],
  "surroundingPairs": [
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*#\\s*region",
      "end": "^\\s*#\\s*endregion"
    }
  }
}